{
  "name": "react-selection",
  "author": "Gregory Beaver",
  "version": "0.1.0",
  "description": "Make any React-based collection of similar components selectable",
  "main": "build/index.js",
  "keywords": [
    "react",
    "selection",
    "selectable"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node isparta cover --report html --report text node_modules/.bin/_mocha",
    "build": "babel src --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src test",
    "storybook": "start-storybook -p 9001"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.28.2",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.10.0",
    "isparta": "^4.0.0",
    "jsdom": "^9.2.1",
    "mocha": "^2.3.4",
    "should": "^9.0.0",
    "teaspoon": "^6.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cellog/react-selection.git"
  },
  "bugs": {
    "url": "https://github.com/cellog/react-selection/issues"
  },
  "homepage": "https://github.com/cellog/react-selection#readme",
  "license": "MIT"
}
